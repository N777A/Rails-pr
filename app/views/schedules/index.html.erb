<h1>スケジュール管理アプリ　</h1>
<h3>予定一覧</h3>
<p><%= link_to "新規追加", :new_schedule %></p>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>タイトル</th>
            <th>開始日</th>
            <th>終了日</th>
            <th>終日</th>
            <th>更新日時</th>
        </tr>
    </thead>

    <tbody>
        <% @schedules.each do |s| %>
        <tr>
         <td><%= s.id %></td>
         <td><%= s.title %></td>   
         <td><%= s.start_date %></td>
         <td><%= s.end_date %></td>
         <td><%= s.all_day %></td>
         <td><%= s.update_time %></td>
        </tr>
        <% end %>
    </tbody>
</table>

